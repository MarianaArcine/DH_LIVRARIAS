        <label>Outros</label>
        <input type="radio" name="opcao" id="op3"><br><br>

        <label class="cadastro_nascimento">Data de Nascimento:</label>
        <input type="date" name="data" id="data"><br><br>

        <h2 class="cadastro_endereco">Endereço</h2><br><br>

        <label class="cadastro_estado">Estado:</label>
        <select name="estado" id="uf">
          <option value="0">Escolha sua Região</option>

