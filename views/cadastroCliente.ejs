<%- include("./partials/header") %>



  <div class="cadastro">
    <div class="cadastro_dados">
      <form method="post" action="index.php"><br>
        <h2 class="cadastro_titulos">Cadastro</h2><br><br>

        <label class="cadastro_nome">Nome:</label>
        <input type="text" size="40" placeholder="Nome Completo">

        <label class="cadastro_cpf">CPF:</label>
        <input type="text" id="cpf" minlength="11" maxlength="11" required autocomplete="off"
          placeholder="231.231.321-00"><br><br>

        <label class="cadastro_email">Email:</label>
        <input  type="email" size="40" placeholder="Email Válido" required>

        <label class="cadastro_rg">RG:</label>
        <input type="text" id="rg" minlength="11" maxlength="11"
          pattern="^[0-9]{3}.?[0-9]{3}.?[0-9]{3}-?[0-9]{2}" required autocomplete="off"
          placeholder="147.147.147-00"><br><br>

       
        <label class="cadastro_genero">Sexo:</label> &nbsp;
        <label>Masculino</label>
        <input type="radio" name="opcao" id="op1">&nbsp;

