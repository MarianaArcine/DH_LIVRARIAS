<%- include("./partials/header") %>

    <div class="container">

        <div class="cadastro_cliente">
            <h2>Crie sua conta</h2>

            <div class="already-user">
                <span  class="ng-scope">Já tem uma conta?</span>
                <a class="login-link ng-binding">Faça seu login</a>
            </div>

            <form action="/cadastroEndereco" method="GET">

                <div class="cadastro">
                    <label for="nome" class="form-label">Nome</label>
                    <input type="text" class="form-control" name="nome" id="nome" />
                    <div class="invalid-feedback"></div>
                </div>

                <div class="cadastro">
                    <label for="sobrenome" class="form-label">Sobrenome</label>
                    <input type="text" class="form-control" name="sobrenome" id="sobrenome" />
                    <div class="invalid-feedback"></div>
                </div>

                <div class="cadastro">
                    <label for="email" class="form-label">E-mail</label>
                    <input type="text" class="form-control" name="email" id="email" />
                    <div class="invalid-feedback"></div>
                </div>

                <div class="cadastro">
                    <label for="email" class="form-label">Confirme seu e-mail</label>
                    <input type="text" class="form-control" name="confirmeEmail" id="confirmeEmail" />
                    <div class="invalid-feedback"></div>
                </div>

                <div class="cadastro">
                    <label for="senha" class="form-label">Senha</label>
                    <input type="password" class="form-control" name="senha" id="senha" />
                    <div class="invalid-feedback"></div>
                </div>

                <div class="cadastro">
                    <label for="confirmeSenha" class="form-label">Confirme sua senha</label>
                    <input type="password" class="form-control" name="confirmeSenha" id="confirmeSenha" />
                    <div class="invalid-feedback"></div>
                </div>

                <div class="btn-cadastro">
                    <button type="submit" class="btn btn-primary">Cadastrar</button>
                </div>

            </form>
        </div>
    </div>


    <%- include("./partials/footer") %>